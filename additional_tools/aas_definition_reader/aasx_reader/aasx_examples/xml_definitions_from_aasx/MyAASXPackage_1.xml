<?xml version='1.0' encoding='UTF-8'?>
<aas:environment xmlns:aas="https://admin-shell.io/aas/3/0">
    <aas:assetAdministrationShells>
        <aas:assetAdministrationShell>
            <aas:id>https://acplt.org/Simple_AAS</aas:id>
            <aas:assetInformation>
                <aas:assetKind>Instance</aas:assetKind>
                <aas:globalAssetId>http://acplt.org/Simple_Asset</aas:globalAssetId>
            </aas:assetInformation>
            <aas:submodels>
                <aas:reference>
                    <aas:type>ModelReference</aas:type>
                    <aas:keys>
                        <aas:key>
                            <aas:type>Submodel</aas:type>
                            <aas:value>https://acplt.org/Simple_Submodel</aas:value>
                        </aas:key>
                    </aas:keys>
                </aas:reference>
            </aas:submodels>
        </aas:assetAdministrationShell>
    </aas:assetAdministrationShells>
    <aas:submodels>
        <aas:submodel>
            <aas:id>https://acplt.org/Simple_Submodel</aas:id>
            <aas:kind>Instance</aas:kind>
            <aas:submodelElements>
                <aas:file>
                    <aas:idShort>documentationFile</aas:idShort>
                    <aas:value>/aasx/suppl/MyExampleFile.pdf</aas:value>
                    <aas:contentType>application/pdf</aas:contentType>
                </aas:file>
            </aas:submodelElements>
        </aas:submodel>
    </aas:submodels>
</aas:environment>